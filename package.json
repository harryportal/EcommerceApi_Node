{
  "devDependencies": {
    "@types/express": "^4.17.15",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.15",
    "dotenv-cli": "^6.0.0",
    "express": "^4.18.2",
    "express-validator": "^6.14.2",
    "express-validators": "^1.0.4",
    "jest": "^29.3.1",
    "prisma": "^4.7.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon -L source/index.ts",
    "docker:up": "docker-compose up -d",
    "docker:logs": "docker-compose logs -f backend",
    "docker:down": "docker-compose down",
    "dev:migrate": "dotenv -e .env.development -- npx prisma migrate dev --name init",
    "test:migrate": "dotenv -e .env.test -- npx prisma migrate dev --name init",
    "migrate": "docker-compose exec backend npm run dev:migrate",
    "migrate:test": "docker-compose exec backend npm run test:migrate",
    "docker:test": "dotenv -e .env.test -- npx jest --watchAll",
    "test": "docker-compose exec backend npm run docker:test"
  },
  "dependencies": {
    "@prisma/client": "^4.8.1",
    "@types/bcryptjs": "^2.4.2",
    "axios": "^1.2.2",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "express-async-errors": "^3.1.1",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.20"
  }
}
